(define (user-won?)
  (and
   (eq? (#js.document.getElementById().textContent) word)
   (<= guesses max-guesses)))

(define (user-lost?)
  (> guesses max-guesses))

(define (get-random-word)
  (list-ref names (random (length names))))

(define (hide-word word)
  (string-join 
   (for/list ([letter word]) "_ ")))

(define (letter? l)
  (or (char-alphabetic? l) (char-numeric? l) (char-punctuation? l)))

(define (correct-guess word letter)
  (and (letter? l) 
       (string-contains? word letter)))

(define (wrong-guess?)
  (not (correct-guess?)))

